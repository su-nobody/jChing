    if (l3ne.classList.contains('yin') || l3ne.classList.contains('yinC'))
      if (l4ne.classList.contains('yang') || l4ne.classList.contains('yangC'))
		if (l5ne.classList.contains('yang') || l5ne.classList.contains('yangC'))
			if (l6ne.classList.contains('yin') || l6ne.classList.contains('yinC'))
{
  document.getElementById('hex57').style.display = "block";
}

if (l1ne.classList.contains('yin') || l1ne.classList.contains('yinC'))
  if (l2ne.classList.contains('yang') || l2ne.classList.contains('yangC'))
    if (l3ne.classList.contains('yang') || l3ne.classList.contains('yangC'))
