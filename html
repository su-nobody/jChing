function castStalks() {
var lines = [ 'yang', 'yang', 'yang', 'yang', 'yang', 'yin', 'yin', 'yin', 'yin', 'yin', 'yin', 'yin', 'yangC', 'yangC', 'yangC', 'yinC' ];
var l1 = Math.floor(Math.random() * lines.length);
var l2 = Math.floor(Math.random() * lines.length);
var l3 = Math.floor(Math.random() * lines.length);
var l4 = Math.floor(Math.random() * lines.length);
var l5 = Math.floor(Math.random() * lines.length);
var l6 = Math.floor(Math.random() * lines.length);
var l1ne = document.getElementById('1line');
var l2ne = document.getElementById('2line');
var l3ne = document.getElementById('3line');
var l4ne = document.getElementById('4line');
var l5ne = document.getElementById('5line');
var l6ne = document.getElementById('6line');


l1ne.classList.remove('yin');
l2ne.classList.remove('yin');
l3ne.classList.remove('yin');
l4ne.classList.remove('yin');
l5ne.classList.remove('yin');
l6ne.classList.remove('yin');
  
l1ne.classList.remove('yiC');
l2ne.classList.remove('yinC');
l3ne.classList.remove('yinC');
l4ne.classList.remove('yinC');
l5ne.classList.remove('yinC');
l6ne.classList.remove('yinC');
  
l1ne.classList.remove('yang');
l2ne.classList.remove('yang');
l3ne.classList.remove('yang');
l4ne.classList.remove('yang');
l5ne.classList.remove('yang');
l6ne.classList.remove('yang');
  
l1ne.classList.remove('yangC');
l2ne.classList.remove('yangC');
l3ne.classList.remove('yangC');
l4ne.classList.remove('yangC');
l5ne.classList.remove('yangC');
l6ne.classList.remove('yangC');
  
l1ne.classList.add(lines[l1]);
l2ne.classList.add(lines[l2]);
l3ne.classList.add(lines[l3]);
l4ne.classList.add(lines[l4]);
l5ne.classList.add(lines[l5]);
l6ne.classList.add(lines[l6]);

showResult();

const hexlines = document.querySelectorAll(".line");
for (var x = 0; x < hexlines.length; x++)
    if (hexlines[x].contains = "yinC" || "yangC") {
    	document.getElementById("cast").style.display = "none";
		document.getElementById("transform").style.display = "block";
    }
}



function transform() {
var l1ne = document.getElementById('1line');
var l2ne = document.getElementById('2line');
var l3ne = document.getElementById('3line');
var l4ne = document.getElementById('4line');
var l5ne = document.getElementById('5line');
var l6ne = document.getElementById('6line');
  
if (l1ne.classList.contains('yinC'))
  {
    l1ne.classList.remove('yinC');
    l1ne.classList.add('yang');
  }
if (l1ne.classList.contains('yangC'))
  {
    l1ne.classList.remove('yangC');
    l1ne.classList.add('yin');
  }
  
if (l2ne.classList.contains('yinC'))
  {
    l2ne.classList.remove('yinC');
    l2ne.classList.add('yang');
  }
if (l2ne.classList.contains('yangC'))
  {
    l2ne.classList.remove('yangC');
    l2ne.classList.add('yin');
  }
if (l3ne.classList.contains('yinC'))
  {
    l3ne.classList.remove('yinC');
    l3ne.classList.add('yang');
  }
  
if (l3ne.classList.contains('yangC'))
  {
    l3ne.classList.remove('yangC');
    l3ne.classList.add('yin');
  }
if (l4ne.classList.contains('yinC'))
  {
    l4ne.classList.remove('yinC');
    l4ne.classList.add('yang');
  }
if (l4ne.classList.contains('yangC'))
  {
    l4ne.classList.remove('yangC');
    l4ne.classList.add('yin');
  }
if (l5ne.classList.contains('yinC'))
  {
    l5ne.classList.remove('yinC');
    l5ne.classList.add('yang');
  }
if (l5ne.classList.contains('yangC'))
  {
    l5ne.classList.remove('yangC');
    l5ne.classList.add('yin');
  }
  
if (l6ne.classList.contains('yinC'))
  {
    l6ne.classList.remove('yinC');
    l6ne.classList.add('yang');
  }
if (l6ne.classList.contains('yangC'))
  {
    l6ne.classList.remove('yangC');
    l6ne.classList.add('yin');
  }
 showResult();
document.getElementById("cast").style.display = "block";
document.getElementById("transform").style.display = "none";
}

</script>
